ewMap = new Map()\n        this.menuList.forEach((item) => {\n          this.firstLevel.push({\n            name:item.name\n          })\n          newMap.set(item.name, item.children)\n        })\n        // 菜单map\n        this.menuListMap = newMap\n      }\n    },\n    // 选择一级菜单\n    selectMenu(value){\n      this.secondLevel = this.menuListMap.get(value)\n      console.log(this.secondLevel)\n    },\n    // 选择二级菜单\n \n  }\n}\n",{"version":3,"sources":["Layout.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA","file":"Layout.vue","sourceRoot":"src/views/layout","sourcesContent":["<template>\n  <a-layout id=\"components-layout-demo-custom-trigger\">\n    <a-layout-sider\n      :trigger=\"null\"\n      collapsible\n      v-model=\"collapsed\"\n    >\n      <div class=\"logo\" />\n      <a-menu theme=\"dark\" mode=\"inline\" :defaultSelectedKeys=\"defaultSelectedKeys\">\n        <a-menu-item  @click=\"selectMenu(item.name)\" v-for=\"(item,index) in firstLevel\" :key=\"index\">\n          <svg-icon icon-class=\"password\"/>\n          <span>{{item.name}}</span>\n        </a-menu-item>\n      </a-menu>\n     \n    </a-layout-sider>\n    <a-layout>\n      <a-layout-sider style=\"background: #fff; padding: 0\" v-if=\"!collapsed\">\n        <a-menu\n            mode=\"inline\"\n            theme=\"dark\"\n            :inlineCollapsed=\"collapsed\"\n          >\n          <template v-for=\"item in secondLevel\">\n            <a-menu-item v-if=\"!item.children\" :key=\"item.key\">\n              <router-link :to=\"{name: item.name }\" >\n                 <a-icon type=\"pie-chart\" />\n                 <span>{{item.name}}</span>\n              </router-link>\n            </a-menu-item>\n            <sub-menu v-else :menu-info=\"item\" :key=\"item.key\"/>\n          </template>\n        </a-menu> \n      </a-layout-sider>\n      <a-layout>\n          <a-layout-header style=\"background: #fff; padding: 0\">\n            <div class=\"flexbox\">  \n                <a-icon\n                  class=\"trigger\"\n                  :type=\"collapsed ? 'menu-unfold' : 'menu-fold'\"\n                  @click=\"()=> collapsed = !collapsed\"\n                />\n                <navbar/>\n            </div>\n          </a-layout-header>\n        <a-layout-content :style=\"{ margin: '24px 16px', padding: '24px', background: '#fff', minHeight: '280px' }\">\n          <app-main/>\n        </a-layout-content>\n      </a-layout>\n    </a-layout>\n  </a-layout>\n</template>\n\n<script>\nimport SubMenu from './subMenu'\nimport { Navbar, AppMain } from './components'\n// 获取菜单\nimport {getMenu} from '@/api/login.js'\n\nimport { addRouter } from '@/utils/addRouter'\nexport default {\n  name: 'Layout',\n  computed:{\n    \n    routes() {\n      return this.$store.getters.routerList\n    },\n  },\n  data(){\n    return {\n      defaultSelectedKeys:['1',],\n      // 展开\n      collapsed: false,\n      // 菜单列表\n      menuList:[],\n      // 菜单列表map\n      menuListMap:{},\n      // 一级菜单\n      firstLevel:[],\n      // 二级菜单\n      secondLevel:[]\n    }\n  },\n  components: {\n    Navbar,\n    AppMain,\n    'sub-menu': SubMenu,\n  },\n  created(){\n    this.getAllMenu()\n  },\n  methods: {\n    async getAllMenu(){\n      let result = await getMenu()\n      this.menuList = addRouter(result.data.data.router)\n      console.log(this.menuList)\n      this.firstLevel = []\n      this.menuListMap = {}\n      // 数组对象转map\n      if(this.menuList){\n        let newMap = new Map()\n        this.menuList.forEach((item) => {\n          this.firstLevel.push({\n            name:item.name\n          })\n          newMap.set(item.name, item.children)\n        })\n        // 菜单map\n        this.menuListMap = newMap\n      }\n    },\n    // 选择一级菜单\n    selectMenu(value){\n      this.secondLevel = this.menuListMap.get(value)\n      console.log(this.secondLevel)\n    },\n    // 选择二级菜单\n \n  }\n}\n</script>\n\n<style rel=\"stylesheet/scss\" lang=\"scss\" scoped>\n  #components-layout-demo-custom-trigger .trigger {\n    font-size: 18px;\n    line-height: 64px;\n    padding: 0 24px;\n    cursor: pointer;\n    transition: color .3s;\n  }\n\n  #components-layout-demo-custom-trigger .trigger:hover {\n    color: #1890ff;\n  }\n\n  #components-layout-demo-custom-trigger .logo {\n    height: 32px;\n    background: rgba(255,255,255,.2);\n    margin: 16px;\n  }\n  .flexbox{\n    display: flex;\n    width:100%;\n    align-items: center;\n    justify-content:space-between;\n  }\n</style>\n"]}]}